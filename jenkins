pipeline {
	agent any
	stages {
		stage ('BUILD') {
			steps {
			sh '''
			sleep 3
			echo "this is build"
			'''
			}
			}
		stage ('DEPLOY) {
			steps {
			sh '''
			sleep 3
			echo "this is deploy"
			'''
			}
			}
		stage ('TEST') {
			steps {
			sh '''
			sleep 3
			echo "this is test phase"
			'''
			}
			}
			}
			}
pipeline {
  agent any
    stages {
      stage ('BUILD') {
        steps {
          sh '''
          sleep 5
          echo "this is build stage"
          '''
}
}
stage ('DEPLOY') {
  steps {
    sh '''
    sleep 5
    echo "this is deploy stage"
    '''
}
}
stage ('TEST') {
  steps {
    sh '''
    sleep 5
    echo "this is test stage"
    '''
}
}
}
}
